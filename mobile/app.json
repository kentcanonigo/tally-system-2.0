{
  "expo": {
    "name": "Tally System",
    "slug": "tally-system",
    "version": "2.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#db2175"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tallysystemmobile",
      "statusBar": {
        "barStyle": "light-content"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#db2175"
      },
      "package": "com.tallysystemmobile",
      "statusBar": {
        "backgroundColor": "#db2175",
        "barStyle": "light-content"
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "97f07e81-961b-4289-8f2d-19d7e283e272"
      }
    }
  }
}
